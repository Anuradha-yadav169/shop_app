<div class="container">   
  
<%= form_for @address, url: {action: :update} do |f| %>   
     
   <h3>edit</h3>      
    <p>   
  
     <div>   
        <label>First Name</label>   
        <%= f.text_field :first_name %>   
     </div>   
  
     <div>   
        <label>Last Name</label>   
        <%= f.text_field :last_name %>   
     </div>   
        
     <div>   
        <label>City</label>   
        <%= f.text_field :city%>   
     </div>
     <div>   
        <label>Country</label>   
        <%= f.text_field :country %>   
    </div>
    <div>   
        <label>Code</label>   
        <%= f.text_field :postal_code %>   
    </div> 
    <div>   
        <label>Phone</label>   
        <%= f.text_field :phone %>   
    </div>
    <div>   
        <label>Email</label>   
        <%= f.text_field :email %>   
    </div>  
  
    </p>   
   <div>   
      <%= link_to 'Back', { controller: 'addresses', action: 'index'} %>   
      <%= f.submit 'Update Address' %>   
    </div>   
<% end %>   
</div>  